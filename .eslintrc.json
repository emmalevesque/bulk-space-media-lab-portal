{
  "extends": [
    "@sanity/eslint-studio-plugin"
  ],
  "plugins": [
    "simple-import-sort",
    "eslint-plugin-react"
  ],
  "rules": {
    "simple-import-sort/imports": "off",
    "simple-import-sort/exports": "off",
    "react-hooks/exhaustive-deps": "error",
    "import/no-anonymous-default-export": "off",
    "react/display-name": "off"
  }
}