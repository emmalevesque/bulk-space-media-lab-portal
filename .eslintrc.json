{
  "extends": "next",
  "plugins": ["simple-import-sort", "eslint-plugin-react"],
  "rules": {
    "simple-import-sort/imports": "off",
    "simple-import-sort/exports": "off",
    "no-unused-vars": 2,
    "react-hooks/exhaustive-deps": "error",
    "import/no-anonymous-default-export": "off",
    "react/display-name": "off"
  }
}
